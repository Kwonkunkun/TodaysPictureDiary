{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@constants": ["constants"],
      "@types": ["types"],
      "@state": ["state"],
      "@Utils": ["Utils"],
      "@assets/*": ["../assets/*"],
      "@env": [".env"],
      "@service/*": ["service/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
